def port_scan(target_host, target_ports):
    port_protocol = {
        80: "HTTP",
        443: "HTTPS",
        22: "SSH",
        21: "FTP",
        25: "SMTP",
        53: "DNS",
        110: "POP3",
        143: "IMAP",
        3389: "RDP",
        445: "SMB",
        23: "Telnet",
        139: "NetBIOS",
        3306: "MySQL",
        8080: "HTTP",
        1194: "OpenVPN",
        161: "SNMP",
        123: "NTP",
        995: "POP3 SSL",
        587: "SMTP",
        465: "SMTP SSL"
    }
    
    for port in target_ports:
        try:
            sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            
            # Portu kontrol edin lÃ¼tfen.
            result = sock.connect_ex((target_host, port))
            if result == 0:
                protocol = port_protocol.get(port, "Bilinmeyen")
                print(f"Port {port}: AÃ§Ä±k ({protocol})")
            else:
                print(f"Port {port}: KapalÄ±")
            
            sock.close()
        
        except KeyboardInterrupt:
            print("Tarama iÅŸlemi kullanÄ±cÄ± tarafÄ±ndan durduruldu.")
            break
        
        except socket.gaierror:
            print("GeÃ§ersiz bir host adÄ±.")
            break
        
        except socket.error:
            print("BaÄŸlantÄ± hatasÄ±.")
            break

target_ip = input("Bir hedef Domain adresi giriniz(Ã¶rn;www.rwoxxi.com): ")

# Taranacak port listesi
target_ports = [80, 443, 22, 21, 25, 53, 110, 143, 3389, 445, 23, 139, 3306, 8080, 1194, 161, 123, 995, 587, 465]

port_scan(target_ip, target_ports)
